{
  "name": "@paquitosoft/fetcher",
  "version": "1.2.0",
  "private": false,
  "description": "",
  "keywords": [],
  "main": "dist/fetcher.umd.js",
  "module": "dist/fetcher.es6.js",
  "typings": "dist/types/fetcher.d.ts",
  "files": [
    "dist"
  ],
  "author": "PaquitoSoft <paquitosoftware@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaquitoSoft/fetcher"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts --environment BUILD:production",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "npm run lint && npm run test:cov -- --no-cache",
    "docs:build": "typedoc --out docs src/fetcher.ts",
    "docs:deploy": "npm run docs:build && ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "release": "standard-version"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^10.11.0",
    "@types/node-fetch": "^2.6.1",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "colors": "^1.3.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-junit": "^13.0.0",
    "jest-mock-extended": "^2.0.4",
    "jest-unit": "0.0.1",
    "lodash.camelcase": "^4.3.0",
    "msw": "^2.3.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "shelljs": "^0.8.3",
    "standard-version": "^9.3.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.12",
    "typescript": "^4.6.2",
    "xmldom": "^0.6.0"
  },
  "overrides": {
    "lodash": "4.17.21"
  }
}
